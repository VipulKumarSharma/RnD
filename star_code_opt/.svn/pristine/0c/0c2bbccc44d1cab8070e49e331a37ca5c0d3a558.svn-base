package src.beans;

public class Train extends BaseBean{
	
	private static final long serialVersionUID = 1L;
	
	private String journyType;
	private String departureCity;
	private String arrivalCity;
	private String departureDate;
	private String preferredTimeType;
	private String timing;
	private String travelClass;
	private String seatPreferrence_1;
	private String seatPreferrence_2;
	private String seatPreferrence_3;
	private String remarks;
	private Fare fareDetail;
	private boolean isSpecialTrain;
	private String prefTrain;
	private boolean isTatkaalTicket;
	
	public Train(){
		super();
	}

	public String getJournyType() {
		return journyType;
	}

	public void setJournyType(String journyType) {
		this.journyType = journyType;
	}

	public String getDepartureCity() {
		return departureCity;
	}

	public void setDepartureCity(String departureCity) {
		this.departureCity = departureCity;
	}

	public String getArrivalCity() {
		return arrivalCity;
	}

	public void setArrivalCity(String arrivalCity) {
		this.arrivalCity = arrivalCity;
	}

	public String getDepartureDate() {
		return departureDate;
	}

	public void setDepartureDate(String departureDate) {
		this.departureDate = departureDate;
	}

	public String getPreferredTimeType() {
		return preferredTimeType;
	}

	public void setPreferredTimeType(String preferredTimeType) {
		this.preferredTimeType = preferredTimeType;
	}

	public String getTiming() {
		return timing;
	}

	public void setTiming(String timing) {
		this.timing = timing;
	}

	public String getTravelClass() {
		return travelClass;
	}

	public void setTravelClass(String travelClass) {
		this.travelClass = travelClass;
	}

	public String getSeatPreferrence_1() {
		return seatPreferrence_1;
	}

	public void setSeatPreferrence_1(String seatPreferrence_1) {
		this.seatPreferrence_1 = seatPreferrence_1;
	}

	public String getSeatPreferrence_2() {
		return seatPreferrence_2;
	}

	public void setSeatPreferrence_2(String seatPreferrence_2) {
		this.seatPreferrence_2 = seatPreferrence_2;
	}

	public String getSeatPreferrence_3() {
		return seatPreferrence_3;
	}

	public void setSeatPreferrence_3(String seatPreferrence_3) {
		this.seatPreferrence_3 = seatPreferrence_3;
	}

	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public Fare getFareDetail() {
		return fareDetail;
	}

	public void setFareDetail(Fare fareDetail) {
		this.fareDetail = fareDetail;
	}

	public boolean isSpecialTrain() {
		return isSpecialTrain;
	}

	public void setSpecialTrain(boolean isSpecialTrain) {
		this.isSpecialTrain = isSpecialTrain;
	}

	public String getPrefTrain() {
		return prefTrain;
	}

	public void setPrefTrain(String prefTrain) {
		this.prefTrain = prefTrain;
	}

	public boolean isTatkaalTicket() {
		return isTatkaalTicket;
	}

	public void setTatkaalTicket(boolean isTatkaalTicket) {
		this.isTatkaalTicket = isTatkaalTicket;
	}
}
