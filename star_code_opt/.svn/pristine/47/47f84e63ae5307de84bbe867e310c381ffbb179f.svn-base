		<defaultValueExpression ><![CDATA["Destination Wise Report"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SET DATEFORMAT DMY 
			
SELECT UPPER($P!{PARAM_strTableName}.TRAVEL_TO) AS TRAVEL_TO, COUNT(*) 
AS REQUISTION_COUNT FROM $P!{PARAM_strTableName} ,T_TRAVEL_STATUS TS 
			
WHERE 
  CAST(FLOOR(CAST($P!{PARAM_strTableName}.C_DATETIME AS FLOAT))AS DATETIME)
BETWEEN $P{PARAM_strTravelFrom} AND 
			$P{PARAM_strTravelTo} AND  
$P!{PARAM_strTableName}.TRAVEL_ID = TS.TRAVEL_ID AND 
			TS.TRAVEL_TYPE =$P{PARAM_strTravelType} 
AND TS.TRAVEL_STATUS_ID !=  1 
			GROUP BY TRAVEL_TO]]></queryString>

	<field name="TRAVEL_TO" class="java.lang.String"/>
	<field name="REQUISTION_COUNT" class="java.lang.Integer"/>

