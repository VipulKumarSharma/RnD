	<parameter name="PARAM_strTravelType" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["I"]]></defaultValueExpression>
	</parameter>
	<parameter name="PARAM_strTravelFrom" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["01/10/2006"]]></defaultValueExpression>
	</parameter>
	<parameter name="PARAM_strTravelTo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["12/10/2008"]]></defaultValueExpression>
	</parameter>
	<parameter name="PARAM_strTableName" isForPrompting="true" class="java.lang.String">
