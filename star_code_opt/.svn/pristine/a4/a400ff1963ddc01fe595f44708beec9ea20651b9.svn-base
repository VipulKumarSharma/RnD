.bt-RequestType {
	display: block;
	margin: 0;
	padding: 0;
}

.bt-RequestType label {
	cursor: pointer;
	display: block;
	font-size: 11px;
	font-weight: bold;
	height: 15px !important;
	margin: 0 8px 0 0;
	min-width: 42px;
	padding: .3em .5em .3em .5em;
	position: relative;
	text-align: center;
	width: 130px;
}

.bt-RequestType label.Dom {
	background-color: #00a452;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #ffffff;
	border: 2px solid #ffff00;
}

.bt-RequestType label.Int {
	background-color: #0082bf;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #ffffff;
	border: 2px solid #ffff00;
}

.bt-RequestType label.Dom.Disable, .bt-RequestType label.Int.Disable {
	background-color: #e0d9bf;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #8f8f8f;
	border: 1px solid #baab70;
}

.bt-RequestType label.Dom.Disable:hover, .bt-RequestType label.Int.Disable:hover
	{
	background-color: #e0d9bf;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #8f8f8f;
	border: 1px solid #baab70;
}

.bt-RequestType label.Dom:hover {
	background-color: #006f1c;
}

.bt-RequestType label.Dom.active {
	background-color: #ffff80;
	color: #000000;
}

.bt-RequestType label.Int:hover {
	background-color: #004b97;
}

.bt-RequestType label.Int.active {
	background-color: #ffff80;
	color: #000000;
}

.bt-RequestType label input {
	position: absolute;
	-moz-opacity: 0;
	filter: alpha(opacity : 0);
	opacity: 0;
}

