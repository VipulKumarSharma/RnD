.checkbox {
	width: 100%;
	margin: 0;
	padding: 0;
}

.checkbox label {
	background-color: rgb(220, 220, 220);
	cursor: pointer;
	display: block;
	font-size: 11px;
	font-weight: normal;
	height: 14px !important;
	margin: 0;
	min-width: 40px;
	padding: .45em 0 .45em;
	position: relative;
	text-align: center;
	width: 100%;
}

.checkbox label:hover {
	width: 100%;
	background-color: #f07b05;
	color: #ffffff;
}

.checkbox label.no {
	width: 100%;
	background-color: #dcdcdc;
	-ms-border-radius: 0 .25em .25em 0;
	-moz-border-radius: 0 .25em .25em 0;
	-o-border-radius: 0 .25em .25em 0;
	-webkit-border-radius: 0 .25em .25em 0;
	border-radius: 0 .25em .25em 0;
	color: #000000;
}

.checkbox label.no:hover, .checkbox label.no.active {
	width: 100%;
	background-color: #b22222;
	color: #ffffff;
}

.checkbox label.yes {
	width: 100%;
	background-color: #95afb9;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #000000;
}

.checkbox label.yes:hover, .checkbox label.yes.active {
	width: 100%;
	background-color: #006400;
	color: #ffffff;
}

.checkbox label.yes.disable, .checkbox label.no.disable {
	background-color: #e0d9bf;
	-ms-border-radius: .25em 0 0 .25em;
	-moz-border-radius: .25em 0 0 .25em;
	-o-border-radius: .25em 0 0 .25em;
	-webkit-border-radius: .25em 0 0 .25em;
	border-radius: .25em 0 0 .25em;
	color: #ffffff;
}

.checkbox label input {
	position: absolute;
	-moz-opacity: 0;
	filter: alpha(opacity : 0);
	opacity: 0;
}

